<p id="notice"><%= notice %></p>

<h1>Listing Procurements</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Subject</th>
      <th>Anounced date</th>
      <th>Close date</th>
      <th>Control date</th>
      <th>Confirmed</th>
      <th>Confirmed date</th>
      <th>Effective date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @procurements.each do |procurement| %>
      <tr>
        <td><%= procurement.name %></td>
        <td><%= procurement.subject %></td>
        <td><%= procurement.anounced_date %></td>
        <td><%= procurement.close_date %></td>
        <td><%= procurement.control_date %></td>
        <td><%= procurement.confirmed %></td>
        <td><%= procurement.confirmed_date %></td>
        <td><%= procurement.effective_date %></td>
        <td><%= link_to 'Show', procurement %></td>
        <td><%= link_to 'Edit', edit_procurement_path(procurement) %></td>
        <td><%= link_to 'Destroy', procurement, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Procurement', new_procurement_path %>
