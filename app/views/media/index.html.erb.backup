<p id="notice"><%= notice %></p>

<h1>Listing Media</h1>

<table>
  <thead>
    <tr>
      <th>File name</th>
      <th>File type</th>
      <th>File size</th>
      <th>File</th>
      <th>File url</th>
      <th>File path</th>
      <th>Subject</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @media.each do |medium| %>
      <tr>
        <td><%= medium.file_name %></td>
        <td><%= medium.file_type %></td>
        <td><%= medium.file_size %></td>
        <td><%= medium.file %></td>
        <td><%= medium.file_url %></td>
        <td><%= medium.file_path %></td>
        <td><%= medium.subject %></td>
        <td><%= medium.note %></td>
        <td><%= link_to 'Show', medium %></td>
        <td><%= link_to 'Edit', edit_medium_path(medium) %></td>
        <td><%= link_to 'Destroy', medium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Medium', new_medium_path %>
